<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cola.machine.dao.SmsBatchMapper">
  <resultMap id="BaseResultMap" type="cola.machine.bean.SmsBatch">
      <id column="id" jdbcType="INT" property="id" />
      <result column="total" jdbcType="INT" property="total" />
      <result column="succ" jdbcType="INT" property="succ" />
      <result column="fail" jdbcType="INT" property="fail" />
      <result column="status" jdbcType="INT" property="status" />
      <result column="phone" jdbcType="LONGVARCHAR" property="phone" />
      <result column="content" jdbcType="VARCHAR" property="content" />
      <result column="sendTime" jdbcType="TIMESTAMP" property="sendTime" />
  </resultMap>
  <sql id="Base_Column_List">
    id ,total ,succ ,fail ,status ,phone ,content ,sendTime 
   
  </sql>
  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from SmsBatch
    where id =   #{id,jdbcType=INT}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from SmsBatch
    where id = #{id,jdbcType=INT}
  </delete>
 
  
  <select id="selectBeanByBean" parameterType="BaseResultMap" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from SmsBatch where 1=1
        <if test="id != null and id != '' ">  
           and id = #{id}
        </if>  
        <if test="total != null and total != '' ">  
           and total = #{total}
        </if>  
        <if test="succ != null and succ != '' ">  
           and succ = #{succ}
        </if>  
        <if test="fail != null and fail != '' ">  
           and fail = #{fail}
        </if>  
        <if test="status != null and status != '' ">  
           and status = #{status}
        </if>  
        <if test="phone != null and phone != '' ">  
           and phone = #{phone}
        </if>  
        <if test="content != null and content != '' ">  
           and content = #{content}
        </if>  
        <if test="sendTime != null and sendTime != '' ">  
           and sendTime = #{sendTime}
        </if>  
  </select>
</mapper>
